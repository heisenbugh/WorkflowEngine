@using WorkflowEngine.Core.Dtos;
@model IList<HasanUserDto>

<div>
    @if (Model != null)
    {
        foreach (var hasanUserDto in Model)
        {
            <text>
                @(hasanUserDto.Id) : @(hasanUserDto.FirstName) : @(hasanUserDto.LastName)
                <br />
            </text>
        }
    }

</div>
